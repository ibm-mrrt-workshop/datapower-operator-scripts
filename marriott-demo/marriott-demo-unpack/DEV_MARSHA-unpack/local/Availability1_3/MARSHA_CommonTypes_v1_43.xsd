			<xs:element name="SMS" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="Language" type="xs:language"/>
					<xs:attribute name="CustomerConsent" type="xs:boolean"/>
					<xs:attribute name="PhoneNumber" type="xs:string" use="required"/>
					<xs:attribute name="CountryAccessCode" type="StringLength10"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
